  "license": "MIT",
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.1",
    "glslify-loader": "^2.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.27",
    "postcss-loader": "^7.3.3",
    "raw-loader": "^4.0.2",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "normalize-wheel": "^1.0.1",
    "ogl-typescript": "^0.1.40",
    "pug": "^3.0.2"
  }
}
