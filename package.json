{
  "name": "alxoilerplate",
  "version": "1.0.0",
  "packageManager": "yarn@3.6.0",
  "main": "app.js",
  "scripts": {
    "build:prod": "webpack --progress --config webpack.config.prod.js",
    "build:dev": "webpack serve --progress --stats-error-details --config webpack.config.dev.js",
    "dev": "concurrently --kill-others \"nodemon app.ts\" \"yarn build:dev\""
  },
